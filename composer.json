{
    "name": "byteam/spectre",
    "description": "Lumen security module",
    "version": "0.9.0-dev",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.1.3",
        "laravel/lumen-framework": "5.6.*",
        "league/oauth2-server": "^7.2",
        "symfony/psr-http-message-bridge": "^1.0",
        "vlucas/phpdotenv": "~2.2",
        "zendframework/zend-diactoros": "^1.8"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Byteam\\Spectre\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Byteam\\Spectre\\Providers\\SpectreServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
