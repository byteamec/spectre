{
    "name": "byteam/spectre",
    "description": "Lumen security module",
    "version": "0.9.0-dev",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.0.2",
        "illuminate/auth": "^9.19",
        "laravel/lumen-framework": "^9.19",
        "league/oauth2-server": "^8.3",
        "nyholm/psr7": "^1.5",
        "rennokki/laravel-eloquent-query-cache": "^3.0",
        "symfony/psr-http-message-bridge": "^2.1",
        "vlucas/phpdotenv": "^5.4.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Byteam\\Spectre\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Byteam\\Spectre\\Providers\\SpectreServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
